{
    "name": "书包网",
    "url": "www.bookbao99.net",
    "version": 101,
    "search": {
        "url": "https://www.bookbao99.net/Search/q_${key}",
        "list": "div.search_box > ul > li > div.txt",
        "name": ".t > a:nth-child(1)",
        "author": ".txt > div:nth-child(2) > span:nth-child(1)",
        "cover": ".pic > a > img",
        "detail": ".txt > span > a"
    },
    "detail": {
        "name": "#info > h1",
        "author": "#info > p:nth-child(2) > a",
        "cover": "#fmimg > img",
        "summary": "div:nth-child(5) > div.infocontent"
    },
    "catalog": {
        "list": "div:nth-child(7) > ul",
        "name": "div:nth-child(7) > ul > li:nth-child(1) > a",
        "chapter": "div:nth-child(7) > ul > li:nth-child(1) > a"
    },
    "chapter": {
        "content": "#contents"
    }
}
